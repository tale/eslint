{
	"name": "eslint-config-tale",
	"version": "1.0.5",
	"description": "Aarnav Tale's personal ESLint Configuration",
	"author": "Aarnav Tale <tale@chariz.com>",
	"main": "./dist/eslint.js",
	"module": "./dist/eslint.mjs",
	"exports": {
		".": {
			"import": "./dist/eslint.mjs",
			"require": "./dist/eslint.js"
		}
	},
	"license": "Apache-2.0",
	"scripts": {
		"push": "tsc && tsup && np --no-cleanup --no-tests --message \"chore: v%s\""
	},
	"keywords": [
		"eslint",
		"eslintconfig",
		"esm",
		"react",
		"preact",
		"typescript",
		"es2022",
		"es2021",
		"es2020",
		"es2019",
		"es2018",
		"es2017",
		"es2016",
		"es2015",
		"es6",
		"es7",
		"es8"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tale/eslint.git"
	},
	"bugs": {
		"url": "https://github.com/tale/eslint/issues"
	},
	"homepage": "https://github.com/tale/eslint#readme",
	"devDependencies": {
		"@types/eslint": "^8.4.2",
		"@types/node": "^17.0.40",
		"np": "^7.6.1",
		"tsup": "^6.1.0",
		"typescript": "^4.7.3"
	},
	"peerDependencies": {
		"eslint": ">= 8",
		"typescript": "*"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"eslint": "^8.17.0",
		"eslint-config-xo": "^0.41.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-config-xo-typescript": "^0.51.1",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unicorn": "^42.0.0"
	}
}
